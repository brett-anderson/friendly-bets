<div class="message">
  {{#if showProfile}}
    <img class="img-rounded" src="{{message.sender.picture}}" />
    <p class="message-sender">
      {{message.chatName}} <span class="timestamp">{{moment-from-now message.timestamp}}</span> 
    </p>
  {{/if}}
  <p class="message-text">
    {{#emoji-tag}}
    {{message.text}}
    {{/emoji-tag}}

  </p>
</div>
